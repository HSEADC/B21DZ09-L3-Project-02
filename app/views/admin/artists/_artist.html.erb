<div id="<%= dom_id artist %>", class="outer-card">
  <%= image_tag artist.post_image.url, class: "image-outer"  %>
  <p>
    <strong>Name:</strong>
    <%= artist.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= artist.description %>
  </p>

  <p>
  <strong>Added by</strong>
  <%= artist.user.email %>
  </p>

</div>
