<p style="color: green"><%= notice %></p>

<h1>Artists</h1>

  <%= link_to "New artist", new_admin_artist_path, class: "link-normal" %>


<div id="artists">
  <% @artists.each do |artist| %>
    <%= render artist %>
    <p>
      <%= link_to "Show this artist", [:admin, artist], class: "link-normal" %>
    </p>
  <% end %>
</div>
