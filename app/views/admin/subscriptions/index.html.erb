<p style="color: green"><%= notice %></p>

<h1>Subscriptions</h1>

<div id="subscriptions">
  <% @subscriptions.each do |subscription| %>
    <%= render subscription %>
    <p>
      <%= link_to "Show this subscription", [:admin, subscription], class: "link-normal wrapped-action-button center-text narrow", style: "margin-bottom: 50px;" %>
    </p>
  <% end %>
</div>

<%= link_to "New subscription", new_admin_subscription_path, class: "link-normal" %>
